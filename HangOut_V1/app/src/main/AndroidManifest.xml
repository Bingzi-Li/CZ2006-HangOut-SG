<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.hangout_v0">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:usesCleartextTraffic="true"
        android:theme="@style/AppTheme">

        <activity android:name=".Login.SignUp"></activity>
        <activity android:name=".ShopDetail.ShopInDetail" />
        <activity android:name=".UserMainActivity" />

        <activity android:name=".Vendor.VendorMainActivity" />
        <activity android:name=".Vendor.AddShop" />
        <activity android:name=".Vendor.EditShop" />
        <activity android:name=".Vendor.VendorShop" />
        <activity android:name=".Vendor.ReservationPage" />

        <activity android:name=".Me.plan.PlanDetailActivity" />
        <activity android:name=".Me.plan.PlanHistoryActivity" />
        <activity android:name=".Me.avator.UserProfile" />
        <activity android:name=".Me.avator.EditCustomerProfile" />


        <activity android:name=".Login.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>