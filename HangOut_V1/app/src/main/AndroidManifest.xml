<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.hangout_v0">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/app_logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/app_logo"
        android:supportsRtl="true"
        android:usesCleartextTraffic="true"
        android:theme="@style/AppTheme"
        >

        <activity android:name=".Login.SignUpAsCustomer"/>
        <activity android:name=".Login.SignUpAsVendor"/>
        <activity android:name=".ShopDetail.ShopInDetail" />
        <activity android:name=".ShopDetail.AddPlan" />
        <activity android:name=".UserMainActivity"
            />


        <activity android:name=".Vendor.VendorMainActivity" />
        <activity android:name=".Vendor.AddShop" />
        <activity android:name=".Vendor.EditShop" />
        <activity android:name=".Vendor.VendorShop" />
        <activity android:name=".Vendor.ReservationPage" />

        <activity android:name=".Me.plan.PlanDetailActivity" />
        <activity android:name=".Me.plan.PlanHistoryActivity" />
        <activity android:name=".Me.avator.UserProfile" />
        <activity android:name=".Me.MeMoreActivity" />
        <activity android:name=".Me.avator.EditCustomerProfile" />
        <activity android:name=".Me.reservation.ReservationActivity"/>


        <activity android:name=".Me.MeHelpCenter"/>
        <activity android:name=".welcome_page.IntroActivity"
            android:theme="@style/AppTheme.NoActionBar"/>

        <activity android:name=".Login.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>